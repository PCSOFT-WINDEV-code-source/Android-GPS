  procedures :
   -
     name : _ChangementEtatGPS
     procedure_id : 2014511348871533330
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //_ChangementEtatGPS (<nEtat>)
      //
      // Paramètres :
      // 	nEtat : <indiquez ici le rôle de nEtat>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE _ChangementEtatGPS(nEtat)
      
      // Ignore l'appel si l'état n'a pas été modifié
      si gnEtatActuel = nEtat ALORS
      	retour
      fin
      
      // Mémorise l'état en cours
      gnEtatActuel = nEtat
      
      
      selon nEtat
      	cas gpsActivé // le fournisseur a été activé par l’utilisateur.
      		LIB_Etat = "GPS Activé"
      		// Demande à suivre le déplacement avec un temps maximum entre deux appels de 
      		GPSSuitDéplacement(_RécupèrePosition,1000)
      	CAS gpsDésactivé // le fournisseur a été désactivé par l’utilisateur.
      		LIB_Etat = "GPS Désactivé"
      	CAS gpsHorsService // le fournisseur est hors-service.
      		LIB_Etat = "GPS hors-service"
      	CAS gpsIndisponible // le fournisseur est temporairement indisponible.
      		LIB_Etat = "GPS Indisponible"
      	CAS gpsDisponible // le fournisseur est disponible.
      		LIB_Etat = "GPS disponible"
      		// Demande à suivre le déplacement avec un temps maximum entre deux appels de 
      		GPSSuitDéplacement(_RécupèrePosition,1000)
      FIN
     type : 458752
   -
     name : _RécupèrePosition
     procedure_id : 2014513440520700266
     type_code : 14
     code : |1-
      // Résumé : Cette procédure est appelée automatiquement par la fonction WLangage GPSSuitDéplacement pour notifier de la nouvelle position
      // Syntaxe :
      //_RécupèrePosition (<MaPosition> est géoPosition)
      //
      // Paramètres :
      //	MaPosition (géoPosition) : La nouvelle position fournie par le GPS
      
      PROCEDURE _RécupèrePosition(MaPosition est un géoPosition)
      
      // Mise à jour des informations sur la position
      // Latitude et longitude
      LIB_Latitude = chaineconstruit("Latitude : %1",MaPosition..Latitude)
      LIB_Longitude = chaineconstruit("Longitude : %1",MaPosition..Longitude)
      
      // Vitesse
      si MaPosition.VitesseValide ALORS
      	LIB_Vitesse	= chaineconstruit("Vitesse : %1 m/s",MaPosition..Vitesse)
      	IMG_VALIDITE_VITESSE = IMG_OK
      sinon
      	LIB_Vitesse	= "Vitesse : N/A"
      	IMG_VALIDITE_VITESSE = IMG_PASOK
      FIN
      
      // Altitude
      SI MaPosition.AltitudeValide ALORS
      	LIB_Altitude	= ChaîneConstruit("Altitude : %1 m",MaPosition..Altitude)
      	IMG_VALIDITE_ALTITUDE = IMG_OK
      SINON
      	LIB_Altitude	= "Altitude : N/A"
      	IMG_VALIDITE_ALTITUDE = IMG_PASOK
      FIN
      
      // Direction
      SI MaPosition.DirectionValide ALORS
      	LIB_Direction	= ChaîneConstruit("Direction : %1 ° E",MaPosition..Direction)
      	IMG_VALIDITE_DIRECTION = IMG_OK
      SINON
      	LIB_DIRECTION	= "Direction : N/A"
      	IMG_VALIDITE_DIRECTION = IMG_PASOK
      FIN
      
      LIB_Dernière_mise_à_jour = ChaîneConstruit("Dernière mise à jour le %1 à %2.",dateverschaine(MaPosition.datemesure..PartieDate),heureverschaine(MaPosition.datemesure..PartieHeure))
      LIB_Dernière_mise_à_jour..Visible = vrai
     type : 458752
